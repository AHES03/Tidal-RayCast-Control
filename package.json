{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tidal-controls",
  "title": "Tidal-Controls",
  "description": "Control playback for Tidal",
  "icon": "extension-icon.png",
  "author": "AHES",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "pause",
      "title": "Pause",
      "subtitle": "Pauses track ",
      "description": "Pauses track on Tidal",
      "mode": "no-view"
    }
  ,
    {
      "name": "play",
      "title": "Play",
      "subtitle": "Play on Tidal",
      "description": "",
      "mode": "no-view"
    }
      ,
    {
      "name": "next-track",
      "title": "Next Track",
      "description": "",
      "mode": "no-view"
    }
      ,
    {
      "name": "previous-track",
      "title": "Previous Track",
      "description": "",
      "mode": "no-view"
    }
      ,
    {
      "name": "playpause",
      "title": "PlayPause",
      "description": "",
      "mode": "no-view"
    }
      ,
    {
      "name": "repeat",
      "title": "Repeat",
      "description": "",
      "mode": "no-view"
    }
      ],
  "dependencies": {
    "@raycast/api": "^1.83.1",
    "@raycast/utils": "^1.17.0",
    "run-applescript": "^7.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build --skip-types -e dist -o dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
